{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "minimum": 0
    },
    "serverId": {
      "type": "integer",
      "minimum": 0
    },
    "key": {
      "type": "integer",
      "minimum": 0
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "dataBase": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    }
  },
  "required": ["id", "serverId", "key", "name", "dataBase", "active"],
  "additionalProperties": false
}
